{
  "name": "Python 3.11 Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/bin/python3.11"
  },
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.11"
    }
  },
  "postCreateCommand": "pip install -r requirements.txt"
}
